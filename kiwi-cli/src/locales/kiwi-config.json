{
  "kiwiDir": "src/locales",
  "configFile": "src/locales/kiwi-config.json",
  "srcLang": "zh-CN",
  "distLangs": [
    "en",
    "zh-CN"
  ],
  "translateOptions": {
    "tld": "cn",
    "concurrentLimit": 10,
    "requestOptions": {}
  },
  "excelOptions": {
    "valueIndex": 2
  },
  "importI18N": "import i18n from '@/locales'",
  "exclude": [
    "node_modules",
    "src/locales",
    "src/extract",
    ".ts$",
    "src/translate.js"
  ],
  "include": [
    "src"
  ]
}